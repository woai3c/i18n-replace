{
  "name": "i18n-replace",
  "version": "0.1.0",
  "description": "前端国际化辅助工具——自动替换中文并翻译",
  "bin": {
    "rep": "./bin/replace.js"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint --ext .js test/ src/ bin/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "commit-msg": "node script/verify-commit.js"
    }
  },
  "keywords": [
    "vue",
    "react",
    "i18n",
    "jsx"
  ],
  "license": "MIT",
  "author": "谭光志",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/woai3c/i18n-replace.git"
  },
  "bugs": {
    "url": "https://github.com/woai3c/i18n-replace/issues"
  },
  "homepage": "https://github.com/woai3c/i18n-replace#readme",
  "dependencies": {
    "axios": "^0.20.0",
    "md5": "^2.3.0",
    "progress": "^2.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2"
  }
}
